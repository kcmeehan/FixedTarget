<?xml version="1.0" encoding="utf-8" ?>

<job  datasetSplitting="fileBased" minEvents="1000" maxEvents="10000" simulateSubmission="false" fileListSyntax="xrootd" softLimits="true">

    <command>
        starver SL16a
        root4star -l -b QA.C\(\"$FILELIST\",\"4x5GeVQA_${JOBID}.root\",999999\) &gt;&amp; QA_${JOBID}.log
<!--         root4star -l -b fxtFemtoAnalysis.C\(\"$FILELIST\",\"4x5GeVFemto_NoCentralityCut_${JOBID}.root\",999999\) &gt;&amp; femto_${JOBID}.log --> 
    </command> 

    <SandBox installer="ZIP">
        <Package>
            <File>file:./QA.C</File>
            <File>file:./fxtFemtoAnalysis.C</File>
            <File>file:./StRoot</File>
            <File>file:./.sl64_gcc482</File>
        </Package>
    </SandBox>

    <stdout discard="true" />
        <input URL="catalog:star.bnl.gov?path~/140/,trgsetupname=fixedTarget2015,production=P16ia,filetype=daq_reco_MuDst,storage=nfs,runnumber[]16140033-16140038" nFiles="all" />
        <output fromScratch="*.root" toURL="file:/gpfs/mnt/gpfs01/star/scratch/campbell/fxt/output/" />     <!-- Change Path -->
        <output fromScratch="*.log" toURL="file:/gpfs/mnt/gpfs01/star/scratch/campbell/fxt/logs/" />        <!-- Change Path -->
    <stderr URL="file:/gpfs/mnt/gpfs01/star/scratch/campbell/fxt/logs/$JOBID.err"/>                         <!-- Change Path -->

    <Generator>
        <Location>/gpfs/mnt/gpfs01/star/scratch/campbell/fxt/schedulerFiles</Location>
        <ReportLocation>/gpfs/mnt/gpfs01/star/scratch/campbell/fxt/schedulerFiles</ReportLocation>                   <!-- Change Path -->
    </Generator>
</job>
