<?xml version="1.0" encoding="utf-8" ?>

<!-- PUT THE SCHEDULER FILES SOMEWHERE MORE CONVENIENT --!>
<job maxFilesPerProcess="2" simulateSubmission="false" fileListSyntax="paths">
  <Generator><Location>/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/finalTest/schedFiles/</Location></Generator>

<!-- PUT THE ERROR AND OUTPUT FILES IN A LOG DIRECTORY--!>
<stdout URL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/finalTest/logs/DataExtract_$JOBINDEX.out" />
<stderr URL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/finalTest/logs/DataExtract_$JOBINDEX.err" />


<!-- INTPUT THE FILE LIST OF MUDSTS TO RUN OVER--!>
<input URL="filelist:/star/u/kmeehan/FixedTarget/davisdstmaker/finaldatacollector/davisdstmaker_ForPiondNdyAnalysis/AuAu_4_5_2015_official_runnumberSelected.list" />

<!-- SANDBOX THE CODE TO BE COMPILED--!>
<SandBox installer="ZIP">
  <Package name="sched">
    <File>file:/star/u/kmeehan/FixedTarget/davisdstmaker/finaldatacollector/davisdstmaker_ForPiondNdyAnalysis/RunStDataCollectorMaker.C</File>
    <File>file:/star/u/kmeehan/FixedTarget/davisdstmaker/finaldatacollector/davisdstmaker_ForPiondNdyAnalysis/StRoot/</File>
  </Package>
</SandBox>

<!-- SET THE STAR LIBRARY TO BE USED: SL15e IS NO LONGER ON RCF BUT SL15e_embed CAN STILL BE USED --!>
<command>
starver SL15e

<!-- COMPILE !-->
cons

<!-- Selecting events: For the pion analysis none of these are used and are thus set to -999 the default value -->
set minVr = -999
set maxVr = -999
set minVz = -999
set maxVz = -999
set minVx = -999
set maxVx = -999
set minVy = -999
set maxVy = -999
set beamSpotX = -999
set beamSpotY = -999
set minPrimaryTracks = -999

<!-- SET NEVENTS TO A HIGH VALUE SO ALL EVENTS ARE USED-->
set nEvents = 999999

<!-- RUN THE CODE TO MAKE DAVISDSTS -->
root4star -q -b RunStDataCollectorMaker.C\($nEvents,\"$FILELIST\",\"$SCRATCH/\",\"$JOBID\",$minVr,$maxVr,$minVz,$maxVz,$minVx,$maxVx,$minVy,$maxVy,$beamSpotX,$beamSpotY,$minPrimaryTracks\)

</command>

<!-- SET THE DIRECTORY WHERE THE OUTPUT ROOT FILES WILL BE MOVED-->
<output fromScratch="*.root" toURL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/finalTest/"/>

</job>
