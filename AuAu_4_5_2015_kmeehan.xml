<?xml version="1.0" encoding="utf-8" ?>

<job maxFilesPerProcess="2" simulateSubmission="false" fileListSyntax="xrootd">
  <Generator><Location>/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/noCuts/schedFiles/</Location></Generator>

<stdout URL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/noCuts/logs/DataExtract_$JOBINDEX.out" />
<stderr URL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/noCuts/logs/DataExtract_$JOBINDEX.err" />


<input URL="catalog:star.bnl.gov?path~/140/,trgsetupname=fixedTarget2015,production=P16ia,filetype=daq_reco_MuDst,storage=nfs -limit 0 -onefile" nFiles="all" />

<SandBox installer="ZIP">
  <Package name="sched">
    <File>file:/star/u/kmeehan/FixedTarget/davisdstmaker/stdatacollector/RunStDataCollectorMaker.C</File>
    <File>file:/star/u/kmeehan/FixedTarget/davisdstmaker/stdatacollector/StRoot/</File>
  </Package>
</SandBox>

<command>
starver SL15e 

cons

<!-- Selecting events -->
set minVr = -999
set maxVr = -999
set minVz = -999
set maxVz = -999
set minVx = -999
set maxVx = -999
set minVy = -999
set maxVy = -999
set beamSpotX = -999
set beamSpotY = -999
set minPrimaryTracks = 2

set nEvents = 999999

root4star -q -b RunStDataCollectorMaker.C\($nEvents,\"$FILELIST\",\"$SCRATCH/\",\"$JOBID\",$minVr,$maxVr,$minVz,$maxVz,$minVx,$maxVx,$minVy,$maxVy,$beamSpotX,$beamSpotY,$minPrimaryTracks\)

</command>

<output fromScratch="*.root" toURL="file:/star/data01/pwg/kmeehan/AuAuProd/4_5GeV/davisdsts/noCuts/"/>

</job>
